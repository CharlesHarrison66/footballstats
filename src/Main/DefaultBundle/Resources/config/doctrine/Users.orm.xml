<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Main\DefaultBundle\Entity\Users" table="users">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstname" type="string" column="firstname" length="35" nullable="true"/>
    <field name="lastname" type="string" column="lastname" length="35" nullable="true"/>
    <field name="email" type="string" column="email" length="50" nullable="false"/>
    <field name="username" type="string" column="username" length="20" nullable="false"/>
    <field name="password" type="string" column="password" length="32" nullable="true"/>
    <field name="role" type="string" column="role" length="20" nullable="false"/>
  </entity>
</doctrine-mapping>
